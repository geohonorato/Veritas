{
  "name": "cesupa-dashboard",
  "version": "1.0.0",
  "description": "Dashboard de controle de acesso",
  "main": "src/main.js",
  "scripts": {
    "start": "node server.js",
    "dist": "electron-builder",
    "rebuild": "electron-rebuild"
  },
  "keywords": [],
  "author": "Geovanni Honorato",
  "license": "ISC",
  "dependencies": {
    "@lancedb/lancedb": "^0.22.0",
    "@langchain/core": "^1.1.17",
    "@langchain/groq": "^1.0.3",
    "@xenova/transformers": "^2.17.2",
    "apache-arrow": "^18.1.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "googleapis": "^170.1.0",
    "langchain": "^1.2.14",
    "node-nlp": "^5.0.0-alpha.5",
    "nodemailer": "^7.0.12",
    "open": "^11.0.0",
    "serialport": "^13.0.0",
    "socket.io": "^4.8.3",
    "xlsx": "^0.18.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "electron": "^29.0.0",
    "electron-builder": "^26.0.12"
  },
  "build": {
    "appId": "com.geovanni.cesupadashboard",
    "productName": "Veritas Desktop",
    "directories": {
      "output": "../build"
    },
    "files": [
      "**/*"
    ],
    "extraResources": [
      "database.json",
      "ai"
    ],
    "win": {
      "target": "portable",
      "icon": "build/icon.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/icon.icns"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icon.png"
    }
  }
}
